{
  "name": "gobo-vscode",
  "displayName": "gobo-vscode",
  "description": "Gobo Eiffel",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "eiffel",
        "aliases": ["Eiffel", "e"],
        "extensions": [".e"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eiffel",
        "scopeName": "source.eiffel",
        "path": "./syntaxes/eiffel.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "gobo-vscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
